#!/bin/bash
set -e

case "$1" in
    remove|upgrade|deconfigure)
        if systemctl is-active --quiet rfc2544; then
            systemctl stop rfc2544 || true
        fi
        ;;

    failed-upgrade)
        ;;

    *)
        echo "prerm called with unknown argument: $1" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0
